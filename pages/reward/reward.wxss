/**swiper**/
swiper{
  width: 100%;
  height: 320rpx;
}
swiper image{
  display: block;
  width: 100%;
  height: 100%;
}

/*items-list*/
.items-list{

}
.item{
  overflow: hidden;
  padding: 10rpx;
}
.userlogo{
  float: left;
  width: 160rpx;
  height: 160rpx; 
}
.text{
  margin-left: 170rpx;
  margin-right: 220rpx;
}
.title{
  color: #212121;
  font-size: 50rpx;
  line-height: 1;
}
.discribe{
  font-size: 30rpx;
  color: #9a9a9a;
  line-height: 1.2;
}
.prices-star{
  float: right;
  width: 200rpx;
  height: 160rpx;
}
.star{
  float: right;
  width: 150rpx;
  height: 35rpx;

margin-left:auto;
margin-right:auto;
padding-left:0;
padding-right:0;
box-sizing:0;
font-size:18px;
text-align:center;
text-decoration:none;
line-height:0;
border-radius:0;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#000000;
background-color:#F8F8F8;

}
.star image{
  width: 100%;
  height: 100%;
}
.prices{
  float: right;
  color: #FF9933;
  font-size: 45rpx;
  line-height: 4.5;
}
